(defn verapplam
  (lam [xs]
    ((lam [x] (+ x x))
     (foldl + 0 xs))))

(defn verlet
  (lam [xs]
    (let ([x (foldl + 0 xs)])
      (+ x x))))

(defn verwat
  (lam [xs]
    (foldl + 0 ((lam [qq] qq)
                xs))))

(lam [zs]
  (+ (verapplam zs)
     (+ (verlet zs)
        (verwat zs))))
